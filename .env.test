# 测试环境配置文件
# 此文件包含测试环境的所有环境变量配置

# 应用基础配置
REACT_APP_NAME=云平台微前端系统 (测试环境)
REACT_APP_VERSION=1.0.0-test
REACT_APP_BASE_URL=https://test.example.com
REACT_APP_PORT=443

# 认证配置
REACT_APP_LOGIN_URL=https://test.example.com/login
REACT_APP_LOGOUT_URL=https://test.example.com/login
REACT_APP_TOKEN_KEY=auth_token

# 微前端全局配置
REACT_APP_MF_HOST=https://test.example.com
REACT_APP_CORS_ENABLED=true

# 模板系统配置
REACT_APP_MF_TEMPLATE_PORT=443
REACT_APP_MF_TEMPLATE_HOST=https://test.example.com/mf-template
REACT_APP_MF_TEMPLATE_ENABLED=true

# 营销系统配置
REACT_APP_MF_MARKETING_PORT=443
REACT_APP_MF_MARKETING_HOST=https://test.example.com/mf-marketing
REACT_APP_MF_MARKETING_ENABLED=true

# 财务系统配置
REACT_APP_MF_FINANCE_PORT=443
REACT_APP_MF_FINANCE_HOST=https://test.example.com/mf-finance
REACT_APP_MF_FINANCE_ENABLED=true

# 用户管理系统配置
REACT_APP_MF_USER_PORT=443
REACT_APP_MF_USER_HOST=https://test.example.com/mf-user
REACT_APP_MF_USER_ENABLED=true

# 库存管理系统配置
REACT_APP_MF_INVENTORY_PORT=443
REACT_APP_MF_INVENTORY_HOST=https://test.example.com/mf-inventory
REACT_APP_MF_INVENTORY_ENABLED=true

# 测试环境特定配置
REACT_APP_HOT_RELOAD=false
REACT_APP_SHOW_DEV_TOOLS=true
REACT_APP_ENABLE_MOCK=true
REACT_APP_LOG_LEVEL=info
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_TRACKING=true
REACT_APP_CDN_ENABLED=false
REACT_APP_COMPRESSION_ENABLED=true
REACT_APP_ENABLE_TESTING_TOOLS=true